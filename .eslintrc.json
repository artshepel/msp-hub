{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["plugin:react/recommended", "standard-with-typescript", "prettier"],
  "overrides": [],
  "parserOptions": {
    "project": ["tsconfig.json"],
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "plugins": ["react"],
  "rules": {
    "@typescript-eslint/strict-boolean-expressions": 0,
    "quotes": ["error", "single"]
  }
}
