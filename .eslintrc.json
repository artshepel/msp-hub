{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "project": ["tsconfig.json"],
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "plugins": ["react"],
    "rules": {
        "quotes": ["error", "single"]
    }
}
